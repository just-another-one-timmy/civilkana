var Examinator = function() {
};

// Romaji table.
Examinator.prototype.table0 = ["a", "i", "u", "e", "o"];

// Hiragana table.
Examinator.prototype.table1 = ["あ", "い", "う", "え", "お"];

// Katakana table.
Examinator.prototype.table2 = ["ア", "イ", "ウ", "エ", "オ"];

Examinator.prototype.convertSymbol = function(symbol, fromTable, toTable) {
    for (var i = 0; i < this['table' + fromTable].length; i++) {
        if (this['table' + fromTable][i] == symbol) {
            return this['table' + toTable][i];
        }
    }
};
